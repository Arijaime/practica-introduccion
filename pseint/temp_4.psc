Proceso RestaDeNumeros
	definir num1,num2,num3,num4,num5,resta Como Entero;
	
	Escribir 'Bienvenido a CalcuRest';
	Escribir 'Ingrese el primer numero ';
	leer num1;
	Escribir 'Ingrese el segundo numero ';
	leer num2;
	Escribir 'Ingrese el tercer numero ';
	leer num3;
	Escribir 'Ingrese el cuarto numero ';
	leer num4;
	Escribir 'Ingrese el ultimo numero ';
	leer num5;
	
	resta<- num1-num2-num3-num4-num5;
	Escribir 'el resultado de la operacion es ',resta;
	
FinProceso
