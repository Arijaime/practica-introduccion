Proceso CalcularValoresFuncion
	definir valorEnX,valorDeFuncion como real;
	
	Escribir 'Bienvenido a calcuFunction';
	Escribir 'Defina los valores de X para la funcion f(x)=2x+3x';
	Escribir 'Ingrese valor de x : ';
	leer valorEnX;
	
	valorDeFuncion<-2*valorEnX+valorEnX*3;
	
	Escribir 'el valor de la funcion f(x)=2x+3x para x = ',valorEnX, ' es ',valorDeFuncion;
	Escribir 'Muchas gracias por usar calcuFunction';
FinProceso
